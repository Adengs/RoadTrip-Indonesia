<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.home.notivication.NotivicationHome">

    <LinearLayout
        android:orientation="horizontal"
        android:background="@drawable/shape_walk_through"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/img_panahkiri"
            android:src="@drawable/ic_iconfinder_arrow_right_2561331"
            android:layout_width="18dp"
            android:layout_marginTop="21.26dp"
            android:layout_marginLeft="21dp"
            android:layout_marginBottom="20.26dp"
            android:layout_height="13.47dp"/>
        <TextView
            android:id="@+id/txtnotif"
            android:text="Notification"
            android:textSize="14dp"
            android:textColor="@color/colorText"
            android:layout_marginLeft="21dp"
            android:layout_marginTop="18dp"
            android:layout_marginBottom="19dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="5dp"
        android:background="@drawable/shape_walk_through"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
                            <LinearLayout
                                android:background="@drawable/shape_home_notivication"
                                android:layout_width="32dp"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="28dp"
                                android:layout_height="32dp">
                                <ImageView
                                    android:id="@+id/img_notiv"
                                    android:layout_margin="9dp"
                                    android:src="@drawable/ic_notif_white"
                                    android:layout_width="14dp"
                                    android:layout_height="14dp"/>
                            </LinearLayout>
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_marginTop="24dp"
                                android:layout_marginLeft="35dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:id="@+id/txtvoting"
                                    android:text="Voting periode 2020 -2025"
                                    android:textSize="14dp"
                                    android:textColor="@color/colorText"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:id="@+id/txtdescvoting"
                                    android:text="In publishing and graphic design"
                                    android:textSize="12dp"
                                    android:textColor="@color/colorDesc"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
            <TextView
                android:id="@+id/txtmin"
                android:text="10 min"
                android:textSize="12dp"
                android:textColor="@color/colorDesc"
                android:layout_marginTop="25dp"
                android:gravity="right"
                android:layout_marginRight="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:id="@+id/txtlines"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="24.5dp"
            android:layout_marginRight="19.5dp"
            android:background="@color/colorCircle"></View>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:background="@drawable/shape_home_notivication"
                android:layout_width="32dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="28dp"
                android:layout_height="32dp">
                <ImageView
                    android:id="@+id/img_notiva"
                    android:layout_margin="9dp"
                    android:src="@drawable/ic_notif_white"
                    android:layout_width="14dp"
                    android:layout_height="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_marginTop="24dp"
                android:layout_marginLeft="35dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txtvoperiod"
                    android:text="Voting periode 2020 -2025"
                    android:textSize="14dp"
                    android:textColor="@color/colorText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:id="@+id/txtdescvoperiod"
                    android:text="In publishing and graphic design"
                    android:textSize="12dp"
                    android:textColor="@color/colorDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <TextView
                android:id="@+id/txtmenit"
                android:text="10 min"
                android:textSize="12dp"
                android:textColor="@color/colorDesc"
                android:layout_marginTop="25dp"
                android:gravity="right"
                android:layout_marginRight="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:id="@+id/txtline"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="24.5dp"
            android:layout_marginRight="19.5dp"
            android:background="@color/colorCircle"></View>
    </LinearLayout>

</LinearLayout>